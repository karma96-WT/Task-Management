generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model taks {
  id          Int      @unique @default(autoincrement())
  title       String
  description String
  duedate     DateTime
  priority    String
}

model user {
  id       Int    @unique @default(autoincrement())
  name     String
  username String
  email    String @unique
  password String
}
